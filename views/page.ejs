<div id='pager'>
    <% if (page.count > 1) { %>
        <% if (page.number >1 ) { %>
            <a id='prev' href='?page=<%= page.number-1 %>'>上一页</a>
        <% } %>
        <% if (page.number < page.count ) { %>
            <a id='next' href='?page=<%= page.number + 1 %>'>下一页</a>
        <% } %>
    <% } %>
</div>
